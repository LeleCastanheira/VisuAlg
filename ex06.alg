algoritmo "ex 06"
var
   numero, sucessor, antecessor: Inteiro
inicio
      Escreva("Digite um número inteiro: ")
      Leia(numero)
      antecessor <- numero - 1
      sucessor <- numero + 1
      Escreval("O antecessor de",numero, " é:",antecessor)
      Escreval("O sucessor de",numero," é:",sucessor)
fimalgoritmo
